{
    "name": "snapshotpl/status-code",
    "description": "HTTP status code value object implementation in PHP",
    "type": "library",
    "keywords": [
        "http",
        "status code",
        "psr-7",
        "value object"
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "zendframework/zend-diactoros": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Snapshotpl\\StatusCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snapshotpl\\StatusCode\\Test\\": "test/"
        }
    }
}
